<?xml version="1.0" encoding="utf-8"?>
<com.yqq.dictwoapp.myview.MyTabHost xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@android:id/tabhost"
    tools:context="com.yqq.dictwoapp.MainActivity">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="match_parent">
        <include layout="@layout/tab_title"/>
        <FrameLayout
            android:id="@android:id/tabcontent"
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="0dip" />
        <TabWidget
            android:showDividers="none"
            android:background="#0073ff"
            android:id="@android:id/tabs"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />
    </LinearLayout>

</com.yqq.dictwoapp.myview.MyTabHost>
